{
  "manifest_version": 3,
  "name": "Smart Redirector",
  "permissions": [
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "*://nytimes.com/*" // Replace with the URL pattern you want to redirect from
  ],
  "background": {
    "service_worker": "scripts/background.js"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["*://archive.is/*"]
    }
  ],
  "version": "1.0"
}
